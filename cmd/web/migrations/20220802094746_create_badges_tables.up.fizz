create_table("nba_badges", {timestamps: false}) {
    t.Column("badge_id", "integer", {primary: true})
    t.Column("name", "string", {"default": ""})
    t.Column("type", "string", {"default": "", "null": true})
    t.Column("info", "string", {"default": "", "null": true})
    t.Column("bronze_url", "string", {"default": "", "null": true})
    t.Column("silver_url", "string", {"default": "", "null": true})
    t.Column("gold_url", "string", {"default": "", "null": true})
    t.Column("hof_url", "string", {"default": "", "null": true})
}