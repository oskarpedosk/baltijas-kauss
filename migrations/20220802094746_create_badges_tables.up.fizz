create_table("badges") {
    t.Column("badge_id", "integer", {primary: true})
    t.Column("name", "string", {"null": true})
    t.Column("type", "string", {"null": true})
    t.Column("info", "string", {"null": true})
    t.Column("img_id", "string", {"default": "default.png"})
    t.Column("url", "string", {"default": "", "null": true})
}