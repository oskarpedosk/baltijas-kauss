create_table("nba22_badges") {
    t.Column("badge_id", "integer", {primary: true})
    t.Column("name", "string", {"default": ""})
    t.Column("type", "string", {"default": ""})
    t.Column("info", "string", {"default": ""})
    t.Column("bronze_url", "string", {"default": ""})
    t.Column("silver_url", "string", {"default": ""})
    t.Column("gold_url", "string", {"default": ""})
    t.Column("hof_url", "string", {"default": ""})
}